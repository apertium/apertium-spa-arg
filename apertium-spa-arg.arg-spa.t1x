  <def-macro n="f_tipusverb" npar="1">
      <choose>
	<when>
	  <test>
	    <equal>
	      <clip pos="1" side="tl" part="a_val"/>
	      <lit-tag v="v2"/>
	    </equal>
	  </test>
	  <let>
	    <var n="valverb"/>
	    <lit v="2"/>
	  </let>
	</when>
      </choose>
      <choose>
<!--	<when>
	  <test>
	    <in caseless="yes">
	      <clip pos="1" side="sl" part="lem"/>
	      <list n="per_de"/>
	    </in>
	  </test>
	  <let>
	    <var n="tipusverb"/>
	    <lit v="vbper_de"/>
	  </let>
	</when>
	-->
	<when>
	  <test>
	    <in caseless="yes">
	      <clip pos="1" side="sl" part="lem"/>
	      <list n="verbs_estat"/>
	    </in>
	  </test>
	  <let>
	    <var n="tipusverb"/>
	    <lit v="vba_en"/>
	  </let>
	</when>
	<when>
	  <test>
	    <in caseless="yes">
	      <clip pos="1" side="sl" part="lem"/>
	      <list n="verbs_moviment"/>
	    </in>
	  </test>
	  <let>
	    <var n="tipusverb"/>
	    <lit v="verbmov"/>
	  </let>
	</when>
      </choose>
      <call-macro n="f_serhi">
	<with-param pos="1"/>
      </call-macro>
    </def-macro>

    <def-macro n="f_hi" npar="1">
      <choose>
	<when>
	  <test>
	    <equal>
	      <clip pos="1" side="sl" part="lem"/>
	      <lit v="bi"/>
	    </equal>
	  </test>
	  <let>
	    <var n="bi"/>
	    <lit v="yes"/>
	  </let>
	</when>
      </choose>
    </def-macro>

    <def-macro n="f_serhi" npar="1">
      <choose>
	<when>
	  <test>
	    <and>
	      <!-- <or> -->
		<equal>
		  <clip pos="1" side="sl" part="a_verb"/>
		  <lit-tag v="vbser"/>
		</equal>
<!--		<equal>
		  <clip pos="1" side="sl" part="lem"/>
		  <lit v="quedar"/>
		</equal>
	      </or>-->
	      <equal>
		<var n="bi"/>
		<lit v="yes"/>
	      </equal>
	    </and>
	  </test>
	  <let>
	    <clip pos="1" side="tl" part="a_verb"/>
	    <lit-tag v="vblex"/>
	  </let>
	  <let>
	    <clip pos="1" side="tl" part="lem"/>
	    <lit v="estar"/>
	  </let>
	</when>
      </choose>
      <let>
	<var n="bi"/>
	<lit v=""/>
      </let>
    </def-macro>

    <def-macro n="f_outverb2" npar="2"><!--Comprueba si hay pronombres que se traducen por nada de aragon�s a espa�ol seg�n el diccionario ("bi" y "en") y env�a el verbo con o sin pronombres de acuerdo con esta comprobaci�n. Esta funci�n es para el caso de verbo + 1 encl�tico. Funci�n necesaria para el correcto colocamiento de la cola en multipalabras con flexi�n intercalada. -->
      <choose>
	<when>
	  <test>
	    <not>
	      <equal>
		<clip pos="2" side="tl" part="lem"/>
		<lit v=""/>
	      </equal>
	    </not>
	  </test>
	  <out>
	    <mlu>
	      <lu>
		<clip pos="1" side="tl" part="lemh"/>
		<clip pos="1" side="tl" part="a_verb"/>
		<clip pos="1" side="tl" part="temps"/>
		<clip pos="1" side="tl" part="persona"/>
		<clip pos="1" side="tl" part="nbr"/>
	      </lu>
	      <lu>
		<clip pos="2" side="tl" part="lem"/>
		<clip pos="2" side="tl" part="a_prnenc"/>
		<clip pos="2" side="tl" part="persona"/>
		<clip pos="2" side="tl" part="gen"/>
		<clip pos="2" side="tl" part="nbr"/>
		<clip pos="1" side="tl" part="lemq"/>
	      </lu>  
	    </mlu>
	  </out>
	</when>
	<otherwise>
	  <call-macro n="f_hi">
	    <with-param pos="1"/>
	  </call-macro>
	  <call-macro n="f_serhi">
	    <with-param pos="1"/>
	  </call-macro>
	  <out>
	    <lu>
	      <clip pos="1" side="tl" part="lemh"/>
	      <clip pos="1" side="tl" part="a_verb"/>
	      <clip pos="1" side="tl" part="temps"/>
	      <clip pos="1" side="tl" part="persona"/>
	      <clip pos="1" side="tl" part="nbr"/>
	      <clip pos="1" side="tl" part="lemq"/>
	    </lu>
	  </out>
	</otherwise>
      </choose>
    </def-macro>

    <def-macro n="f_outverb3" npar="3"><!--Comprueba si hay pronombres que se traducen por nada de catal�n a espa�ol seg�n el diccionario ("hi" y "en") y env�a el verbo con o sin pronombres de acuerdo con esta comprobaci�n. Esta funci�n es para el caso de verbo + 2 encl�ticos.-->
      <choose>
	<when>
	  <test>
	    <not>
	      <equal>
		<clip pos="2" side="tl" part="lem"/>
		<lit v=""/>
	      </equal>
	    </not>
	  </test>
	  <choose>
	    <when>
	      <test>
		<not>
		  <equal>
		    <clip pos="3" side="tl" part="lem"/>
		    <lit v=""/>
		  </equal>
		</not>
	      </test>
	      <out>
		<mlu>
		  <lu>
		    <clip pos="1" side="tl" part="lemh"/>
		    <clip pos="1" side="tl" part="a_verb"/>
		    <clip pos="1" side="tl" part="temps"/>
		    <clip pos="1" side="tl" part="persona"/>
		    <clip pos="1" side="tl" part="nbr"/>
		  </lu>
		  <lu>
		    <clip pos="2" side="tl" part="lem"/>
		    <clip pos="2" side="tl" part="a_prnenc"/>
		    <clip pos="2" side="tl" part="persona"/>
		    <clip pos="2" side="tl" part="gen"/>
		    <clip pos="2" side="tl" part="nbr"/>
		  </lu>
		  <lu>
		    <clip pos="3" side="tl" part="lem"/>
		    <clip pos="3" side="tl" part="a_prnenc"/>
		    <clip pos="3" side="tl" part="persona"/>
		    <clip pos="3" side="tl" part="gen"/>
		    <clip pos="3" side="tl" part="nbr"/>
		    <clip pos="1" side="tl" part="lemq"/>
		  </lu>    
		</mlu>
	      </out>
	    </when>
	    <otherwise>
	      <out>
		<mlu>
		  <lu>
		    <clip pos="1" side="tl" part="lemh"/>
		    <clip pos="1" side="tl" part="a_verb"/>
		    <clip pos="1" side="tl" part="temps"/>
		    <clip pos="1" side="tl" part="persona"/>
