	  </when>
	  <otherwise><!--En caso de que el pronombre no se traduzca, env�a s�lo el verbo -->
	    <out>
	      <lu>
		<get-case-from pos="1">            
		  <clip pos="2" side="tl" part="lemh"/>
		</get-case-from>
		<clip pos="2" side="tl" part="a_verb"/>
		<lit-tag v="ifi"/>
		<clip pos="1" side="tl" part="persona"/>
