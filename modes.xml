  <mode name="an-es" install="yes">
    <pipeline>
      <program name="lt-proc">
        <file name="an-es.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="an-es.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-es-an.an-es.t1x"/>
        <file name="an-es.t1x.bin"/>
        <file name="an-es.autobil.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="an-es.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="an-es.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>


