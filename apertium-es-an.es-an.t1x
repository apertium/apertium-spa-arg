	      <with-param pos="1"/>
	    </call-macro>   
	    <call-macro n="f_pretind"><!--Traduce el pretérito indefinido si es el caso y envía el verbo -->
	      <with-param pos="2"/>
	    </call-macro> 
	  </when>
	  <otherwise><!--Si no concuerdan el pronombre y el verbo, envíalo todo -->
	    <out>
	      <lu>
		<clip pos="1" side="tl" part="whole"/>
	      </lu>
	      <b pos="1"/>
	    </out>
	    <call-macro n="f_pretind">
	      <with-param pos="2"/>
	    </call-macro>
	  </otherwise>
	</choose>
