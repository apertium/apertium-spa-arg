	  <with-param pos="2"/>
	  <with-param pos="1"/>
	</call-macro>  
	<call-macro n="f_lexicAdj">
	  <with-param pos="2"/>
	</call-macro>
	<call-macro n="f_lexicDet">
	  <with-param pos="1"/>
	</call-macro>
	<choose>
	  <when>
	    <test>
	      <and>
		      <in caseless="yes">
		        <clip pos="3" side="sl" part="lem"/>
		        <list n="tiempo"/>
		      </in>
		      <or>
		        <and>
		          <equal caseless="yes">
