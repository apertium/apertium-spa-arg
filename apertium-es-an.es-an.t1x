      <pattern>
	<pattern-item n="prnproref"/>
	<pattern-item n="verbospron"/>
      </pattern>
      <action>
	<choose><!--Si la persona del pronombre y el verbo concuerdan, no envíes el pronombre -->
	  <when>
	    <test>
	      <or>
		<equal>
		  <clip pos="1" side="sl" part="persona"/>
