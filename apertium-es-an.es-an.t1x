	      </and>
	    </test>
	    <let>	      
        <clip pos="2" side="tl" part="a_encpro"/>
	      <lit-tag v="pro"/>
	    </let>
	    <let>	      
        <clip pos="3" side="tl" part="a_encpro"/>
	      <lit-tag v="pro"/>
