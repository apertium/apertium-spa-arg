		    <lit-tag v="sp"/>
		  </equal>
		</not>
	      </and>
	    </test>
	    <let><!--el dinero es - els diners són -->
	      <clip pos="3" side="tl" part="nbr"/>
	      <clip pos="2" side="tl" part="nbr"/>
	    </let>
	  </when>
	</choose>      
		
 	<out>    
 	  <lu>  
 	    <clip pos="1" side="tl" part="whole"/>
 	  </lu> 
 	  <b pos="1"/>
 	  <lu>
	    <clip pos="2" side="tl" part="lemh"/>
