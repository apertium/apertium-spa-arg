      <action>
	<out>
	  <lu>
	    <clip pos="1" side="tl" part="whole"/>
	  </lu>
	  <b pos="1"/>
	  <lu>
	    <lit v="o qual"/>
	    <lit-tag v="rel.an"/>
	    <clip pos="2" side="sl" part="gen"/>
	    <clip pos="2" side="sl" part="nbr"/>
	  </lu>
	</out>

      </action>
    </rule>
